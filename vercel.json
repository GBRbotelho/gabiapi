{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "serverFiles": ["package.json"] }
    }
  ],
  "routes": [
    {
      "src": "index.js",
      "dest": "/",
      "continue": true,
      "headers": {
        "Cache-Control": "public, max- idade=0, deve-revalidar",
        "Service-Worker-Allowed": "/"
      }
    }
  ]
}
